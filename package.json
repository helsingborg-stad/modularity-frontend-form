        "dist/"
    ],
    "browserslist": [
        "> 1%",
        "not dead"
    ],
    "devDependencies": {
        "@biomejs/biome": "^2.3.8",
        "@helsingborg-stad/openstreetmap": "^0.51.4",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "@types/jquery": "^3.5.33",
        "cross-env": "^7.0.3",
        "import-glob-loader": "^1.1.0",
        "jest-environment-jsdom": "^29.7.0",
        "sass": "^1.49.7",
        "ts-jest": "^29.3.2",
        "ts-loader": "^9.5.1",
        "typescript": "^5.5.3"
    },
    "browser": {
        "child_process": false
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
